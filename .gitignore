# Ignore bundler config.
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp

# Ignore rvm configuration.
/.rvmrc

# Ignore project specific files.
/.co2in

# Ignore settings file.
/config/settings.yml

# Ignore secrets file.
/config/secrets.yml

# Ignore public binaries.
/public/CO2IN-Qt.dmg
/public/co2in-0.8.7.1-win32-setup.exe
/public/co2in.conf
